{
  "name": "tt",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jeromebienaime/tt.git",
  "author": "jeromebienaime <jerome.bienaime@bulle-communication.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn serve:front",
    "test:unit": "./node_modules/.bin/ava --verbose",
    "test:cover": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "test:mutation": "./node_modules/.bin/stryker run",
    "test:vulnerable": "./node_modules/.bin/snyk monitor",
    "test:report:coverage": "yarn test && ./node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "test:api:unit": "./node_modules/.bin/ts-node ./node_modules/.bin/tape api/src/**/**.test.ts | node_modules/.bin/tap-spec",
    "test:api:cover": "./node_modules/.bin/nyc yarn test:api:unit",
    "test": "yarn test:unit && yarn test:cover && yarn test:api:unit",
    "lint:js": "./node_modules/.bin/eslint src/ -c ./.eslintrc.json",
    "lint:md": "./node_modules/.bin/remark .",
    "lint": "yarn lint:js & yarn lint:md",
    "check": "yarn lint & yarn test ",
    "serve:front": "./node_modules/.bin/parcel src/index.html --open --no-cache",
    "serve:api:fake": "./node_modules/.bin/json-server --watch fakes/clients.json",
    "serve:api": "php -S localhost:8080 -t api/public",
    "serve:storybook": "start-storybook -p 9009",
    "build:storybook": "build-storybook",
    "generate:app:key": "./node_modules/.bin/password-generator -l 32"
  },
  "browserslist": [
    "last 3 Chrome versions"
  ],
  "dependencies": {
    "@types/faker": "^4.1.9",
    "@types/lodash": "^4.14.149",
    "@types/password-hash": "^1.2.20",
    "@types/tape": "^4.2.34",
    "ava": "^3.3.0",
    "axios": "^0.19.2",
    "chance": "^1.1.4",
    "dayjs": "^1.8.20",
    "dot-prop": "^5.1.1",
    "esm": "^3.2.25",
    "faker": "^4.1.0",
    "is_js": "^0.9.0",
    "jsen": "^0.6.6",
    "json-server": "^0.16.0",
    "lod": "^0.1.0",
    "luxon": "^1.22.2",
    "nock": "^12.0.1",
    "nyc": "^15.0.0",
    "parcel": "^1.12.4",
    "password-generator": "^2.2.3",
    "password-hash": "^1.2.2",
    "react": "^16.8.3",
    "react-dom": "^16.13.0",
    "react-interval": "^2.1.1",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.4.0",
    "reduce-reducers": "^1.0.4",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "redux-diff-logger": "^0.0.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3",
    "restify": "^8.5.1",
    "snyk": "^1.297.2",
    "stryker-cli": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.13",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.13",
    "@stryker-mutator/core": "^2.5.0",
    "@stryker-mutator/html-reporter": "^2.5.0",
    "@stryker-mutator/javascript-mutator": "^2.5.0",
    "@stryker-mutator/jest-runner": "^2.5.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "nyx": "^0.1.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
