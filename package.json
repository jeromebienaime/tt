{
  "name": "tt",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jeromebienaime/tt.git",
  "author": "jeromebienaime <jerome.bienaime@bulle-communication.com>",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/parcel src/index.html --open --no-cache",
    "test:unit": "./node_modules/.bin/ava --verbose",
    "test:cover": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "test:codecov": "./node_modules/.bin/codecov",
    "lint:js": "./node_modules/.bin/eslint src/ -c ./.eslintrc.json",
    "lint:md": "./node_modules/.bin/remark .",
    "lint": "npm run lint:js & npm run lint:md",
    "report-coverage": "./node_modules/.bin/nyc ava --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "ava": "^3.3.0",
    "chance": "^1.1.4",
    "codecov": "^3.6.5",
    "esm": "^3.2.25",
    "nyc": "^15.0.0",
    "parcel": "^1.12.4",
    "react": "^16.8.3",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "reduce-reducers": "^1.0.4",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "redux-diff-logger": "^0.0.9",
    "redux-logger": "^3.0.6",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "nyx": "^0.1.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
