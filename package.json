{
  "name": "tt",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/jeromebienaime/tt.git",
  "author": "jeromebienaime <jerome.bienaime@bulle-communication.com>",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/parcel src/index.html --open --no-cache",
    "test:unit": "./node_modules/.bin/ava --verbose",
    "test:cover": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "test:vulnerable": "./node_modules/.bin/snyk monitor",
    "test": "yarn test:unit && yarn test:cover & yarn test:vulnerable",
    "lint:js": "./node_modules/.bin/eslint src/ -c ./.eslintrc.json",
    "lint:md": "./node_modules/.bin/remark .",
    "lint": "yarn lint:js & yarn lint:md",
    "check": "yarn lint & yarn test ",
    "report-coverage": "yarn test && ./node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "api:fake": "./node_modules/.bin/json-server --watch fakes/clients.json"
  },
  "browserslist": [
    "last 3 Chrome versions"
  ],
  "dependencies": {
    "ava": "^3.3.0",
    "axios": "^0.19.2",
    "chance": "^1.1.4",
    "esm": "^3.2.25",
    "json-server": "^0.16.0",
    "lod": "^0.1.0",
    "luxon": "^1.22.0",
    "nyc": "^15.0.0",
    "parcel": "^1.12.4",
    "password-generator": "^2.2.3",
    "react": "^16.8.3",
    "react-dom": "^16.12.0",
    "react-interval": "^2.1.1",
    "react-redux": "^7.1.3",
    "reduce-reducers": "^1.0.4",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "redux-diff-logger": "^0.0.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3",
    "snyk": "^1.291.0"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "nyx": "^0.1.0"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
